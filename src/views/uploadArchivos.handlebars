 <h1>Subir Documentos</h1>
  
  {{#if folderExists}}
    <form method="post" action="/api/users/{{userId}}/documents" enctype="multipart/form-data">
      <input type="file" name="dni" accept=".pdf, image/*">
      <input type="file" name="addressProof" accept=".pdf, image/*">
      <input type="file" name="bankStatement" accept=".pdf, image/*">
      <button type="submit">Subir Documentos</button>
    </form>
  {{else}}
    <p>La carpeta del usuario no existe. No se pueden cargar documentos.</p>
  {{/if}}
  <script src="/index.js"></script>